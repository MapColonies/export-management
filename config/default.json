{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8087",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "typeOrm": {
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "password": "",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    },
    "database": "raster",
    "schema": "ExportManager",
    "synchronize": false,
    "logging": false,
    "entities": ["**/DAL/entity/**/*.js"],
    "migrations": [],
    "subscribers": ["**/DAL/subscriber/**/*.js"],
    "cli": {
      "entitiesDir": "src/DAL/entity",
      "migrationsDir": "src/DAL/migration",
      "subscribersDir": "src/DAL/subscriber"
    }
  },
  "serviceWebhookEndpoint": "http://localhost:8087/export-tasks/webhook",
  "externalClients": {
    "httpRetry": {
      "attempts": 10,
      "delay": "exponential",
      "shouldResetTimeout": true
    },
    "disableHttpClientLogs": true,
    "exporterTrigger": {
      "url": "http://localhost:8083"
    },
    "jobManager": {
      "url": "http://localhost:8088",
      "exportJobType": "rasterTilesExporter"
    }
  }
}
